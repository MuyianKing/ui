{
  "name": "@muyianking/ui",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://muyianking.github.io/blog/docs/table/home.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MuyianKing/mu-table.git"
  },
  "keywords": [
    "MIT",
    "vue3",
    "ui",
    "typescript"
  ],
  "scripts": {
    "commit": "git add . && git cz",
    "dev": "vite --host",
    "release": "node ./script/publish.js",
    "preview": "vite preview",
    "update-dep": "npm-check-updates & ncu -u & pnpm i",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build": "node ./script/build.js",
    "test": "vite build"
  },
  "dependencies": {
    "element-plus": "^2.9.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@muyianking/build": "^0.0.2",
    "@muyianking/config": "^0.0.8",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.3.2",
    "lint-staged": "^15.4.1",
    "ora": "^8.1.1",
    "typescript": "^5.7.3",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vue-tsc": "^2.2.0",
    "sass": "^1.83.4",
    "@muyianking/components": "workspace:*",
    "@muyianking/theme-chalk": "workspace:*"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}